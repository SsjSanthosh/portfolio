{
  "name": "santhosh-portfolio-blog",
  "private": true,
  "description": "A simplified bare-bones starter for Gatsby",
  "version": "0.1.0",
  "license": "0BSD",
  "scripts": {
    "build": "env-cmd -f .env.production gatsby build",
    "develop": "env-cmd -f .env.development gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1",
    "deploy":"env-cmd -f .env.development gatsby build --prefix-paths && gh-pages -d public"
  },
  "dependencies": {
    "@contentful/rich-text-react-renderer": "^14.1.1",
    "@fortawesome/fontawesome-svg-core": "^1.2.32",
    "@fortawesome/free-brands-svg-icons": "^5.15.1",
    "@fortawesome/free-solid-svg-icons": "^5.15.1",
    "@fortawesome/react-fontawesome": "^0.1.11",
    "env-cmd": "^10.1.0",
    "gatsby": "^2.24.66",
    "gatsby-plugin-react-helmet": "^3.3.12",
    "gatsby-plugin-sass": "^2.3.14",
    "gatsby-plugin-sharp": "^2.6.37",
    "gatsby-remark-images": "^3.3.31",
    "gatsby-remark-relative-images": "^2.0.2",
    "gatsby-source-contentful": "^2.3.48",
    "gatsby-source-filesystem": "^2.3.31",
    "gatsby-transformer-remark": "^2.8.36",
    "gh-pages": "^3.1.0",
    "node-sass": "^4.14.1",
    "react": "^16.12.0",
    "react-accessible-accordion": "^3.3.3",
    "react-dom": "^16.12.0",
    "react-fontawesome": "^1.7.1",
    "react-helmet": "^6.1.0",
    "react-responsive-carousel": "^3.2.10",
    "save-dev": "0.0.1-security"
  },
  "devDependencies": {
    "cross-env": "^7.0.2",
    "prettier": "2.1.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-hello-world"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  }
}
